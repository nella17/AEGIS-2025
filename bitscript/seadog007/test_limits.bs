print("Created square bitmap near limit");

# Test 4: Test bitmap operations at edges
set(b2, 8388606, 0, 1);
int val = get(b2, 8388606, 0);
print(val);

# Test 5: String length limit
# String literals are limited to 255 bytes in tokenizer
string s = "This is a test string that should work fine";
int len = length(s);
print(len);

